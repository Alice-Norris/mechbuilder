		<Sound Name="Mech_Powerdown_TP" res="Sounds/mech_hunchbackiic:hunchbackiic:hunchbackiic_powerdown_tp" offset="0.0,0.0,8.0"/>
		
		<!-- base footstep effect name -->
		<Footstep res="footstep_hunchbackiic" />
