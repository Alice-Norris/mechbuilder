		<AudioBank filename="sounds/mech_hunchbackiic/mech_hunchbackiic.fsb" />
	</PrecacheBanks>
	<SoundList>
		<!-- Names of sounds MUST match enum values in enum_MechSounds.h -->

		<!-- Mech Sounds-->
		<Sound Name="Torso_Twist_Pitch" res="Sounds/mech_hunchbackiic:hunchbackiic:hunchbackiic_pitch" offset="0.0,0.0,7.0"/>
		<Sound Name="Torso_Twist_Yaw" res="Sounds/mech_hunchbackiic:hunchbackiic:hunchbackiic_yaw" offset="0.0,0.0,7.0"/>
		<Sound Name="Mech_Engine" res="Sounds/mech_hunchbackiic:hunchbackiic:hunchbackiic_engine" offset="0.0,0.0,8.0"/>
		<Sound Name="Mech_Powerup" res="Sounds/mech_hunchbackiic:hunchbackiic:hunchbackiic_powerup" offset="0.0,0.0,8.0"/>
		<Sound Name="Mech_Powerdown" res="Sounds/mech_hunchbackiic:hunchbackiic:hunchbackiic_powerdown" offset="0.0,0.0,8.0"/>
