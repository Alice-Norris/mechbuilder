<MechSoundDef sharedpath="Objects/mechs/shared-sounds.xml">
	<PrecacheBanks>
		<AudioBank filename="sounds/mech_jagermech/mech_jagermech.fsb" />
	</PrecacheBanks>
	<SoundList>
		<!-- Names of sounds MUST match enum values in enum_MechSounds.h -->

		<!-- Mech Sounds-->
		<Sound Name="Torso_Twist_Pitch" res="Sounds/mech_jagermech:mech_jagermech:jagermech_pitch" offset="0.0,0.0,9.0"/>
		<Sound Name="Torso_Twist_Yaw" res="Sounds/mech_jagermech:mech_jagermech:jagermech_yaw" offset="0.0,0.0,9.0"/>
		<Sound Name="Mech_Engine" res="Sounds/mech_jagermech:mech_jagermech:jagermech_engine" offset="0.0,0.0,10.5"/>	
		<Sound Name="Mech_Powerup" res="Sounds/mech_jagermech:mech_jagermech:jagermech_powerup" offset="0.0,0.,10.5"/>
		<Sound Name="Mech_Powerdown" res="Sounds/mech_jagermech:mech_jagermech:jagermech_powerdown" offset="0.0,0.0,10.5"/>
		<Sound Name="Cockpit_Immersion" res="Sounds/mech_jagermech:mech_jagermech:jagermech_immersion"/>
		
		<!-- Third Person Sounds -->
		<Sound Name="Torso_Twist_Pitch_TP" res="Sounds/mech_jagermech:mech_jagermech:jagermech_pitch_tp" offset="0.0,0.0,9.0"/>
		<Sound Name="Torso_Twist_Yaw_TP" res="Sounds/mech_jagermech:mech_jagermech:jagermech_yaw_tp" offset="0.0,0.0,9.0"/>
		<Sound Name="Mech_Engine_TP" res="Sounds/mech_jagermech:mech_jagermech:jagermech_engine_tp" offset="0.0,0.0,10.5"/>	
		<Sound Name="Mech_Powerup_TP" res="Sounds/mech_jagermech:mech_jagermech:jagermech_powerup_tp" offset="0.0,0.,10.5"/>
		<Sound Name="Mech_Powerdown_TP" res="Sounds/mech_jagermech:mech_jagermech:jagermech_powerdown_tp" offset="0.0,0.0,10.5"/>
		
		<!-- base footstep effect name -->
		<Footstep res="footstep_jagermech" />
	</SoundList>
</MechSoundDef>
