<MechSoundDef sharedpath="Objects/mechs/shared-sounds.xml">
	<PrecacheBanks>
		<AudioBank filename="sounds/mech_banshee/mech_banshee.fsb" />
	</PrecacheBanks>
	<SoundList>
		<!-- Names of sounds MUST match enum values in enum_MechSounds.h -->

		<!-- Mech Sounds-->
		<Sound Name="Torso_Twist_Pitch" res="Sounds/mech_banshee:mech_banshee:banshee_pitch" offset="0.0,0.0,7.0"/>
		<Sound Name="Torso_Twist_Yaw" res="Sounds/mech_banshee:mech_banshee:banshee_yaw" offset="0.0,0.0,7.0"/>
		<Sound Name="Mech_Engine" res="Sounds/mech_banshee:mech_banshee:banshee_engine" offset="0.0,0.0,8.0"/>
		<Sound Name="Mech_Powerup" res="Sounds/mech_banshee:mech_banshee:banshee_powerup" offset="0.0,0.0,8.0"/>
		<Sound Name="Mech_Powerdown" res="Sounds/mech_banshee:mech_banshee:banshee_powerdown" offset="0.0,0.0,8.0"/>
		
		<!-- Third Person Sounds -->
		<Sound Name="Torso_Twist_Pitch_TP" res="Sounds/mech_banshee:mech_banshee:banshee_pitch_tp" offset="0.0,0.0,8.5"/>
		<Sound Name="Torso_Twist_Yaw_TP" res="Sounds/mech_banshee:mech_banshee:banshee_yaw_tp" offset="0.0,0.0,8.5"/>
		<Sound Name="Mech_Engine_TP" res="Sounds/mech_banshee:mech_banshee:banshee_engine_tp" offset="0.0,0.0,9.5.0"/>
		<Sound Name="Mech_Powerup_TP" res="Sounds/mech_banshee:mech_banshee:banshee_powerup_tp" offset="0.0,0.0,9.5.0"/>
		<Sound Name="Mech_Powerdown_TP" res="Sounds/mech_banshee:mech_banshee:banshee_powerdown_tp" offset="0.0,0.0,9.5.0"/>

		<!-- base footstep effect name -->
		<Footstep res="footstep_banshee" />
	</SoundList>
</MechSoundDef>
