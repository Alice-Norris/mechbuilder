<MechSoundDef sharedpath="Objects/mechs/shared-sounds.xml">
	<PrecacheBanks>
		<AudioBank filename="sounds/mech_enforcer/mech_enforcer.fsb" />
	</PrecacheBanks>
	<SoundList>
		<!-- Names of sounds MUST match enum values in enum_MechSounds.h -->

		<!-- Mech Sounds-->
		<Sound Name="Torso_Twist_Pitch" res="Sounds/mech_enforcer:enforcer:enforcer_pitch" offset="0.0,0.0,7.0"/>
		<Sound Name="Torso_Twist_Yaw" res="Sounds/mech_enforcer:enforcer:enforcer_yaw" offset="0.0,0.0,7.0"/>
		<Sound Name="Mech_Engine" res="Sounds/mech_enforcer:enforcer:enforcer_engine" offset="0.0,0.0,8.0"/>
		<Sound Name="Mech_Powerup" res="Sounds/mech_enforcer:enforcer:enforcer_powerup" offset="0.0,0.0,8.0"/>
		<Sound Name="Mech_Powerdown" res="Sounds/mech_enforcer:enforcer:enforcer_powerdown" offset="0.0,0.0,8.0"/>
		<Sound Name="Cockpit_Immersion" res="Sounds/mech_enforcer:enforcer:enforcer_immersion"/>
		
		<!-- Third Person Sounds -->
		<Sound Name="Torso_Twist_Pitch_TP" res="Sounds/mech_enforcer:enforcer:enforcer_pitch_tp" offset="0.0,0.0,7.0"/>
		<Sound Name="Torso_Twist_Yaw_TP" res="Sounds/mech_enforcer:enforcer:enforcer_yaw_tp" offset="0.0,0.0,7.0"/>
		<Sound Name="Mech_Engine_TP" res="Sounds/mech_enforcer:enforcer:enforcer_engine_tp" offset="0.0,0.0,8.0"/>
		<Sound Name="Mech_Powerup_TP" res="Sounds/mech_enforcer:enforcer:enforcer_powerup_tp" offset="0.0,0.0,8.0"/>
		<Sound Name="Mech_Powerdown_TP" res="Sounds/mech_enforcer:enforcer:enforcer_powerdown_tp" offset="0.0,0.0,8.0"/>
		
		<!-- base footstep effect name -->
		<Footstep res="footstep_enforcer" />
	</SoundList>
</MechSoundDef>